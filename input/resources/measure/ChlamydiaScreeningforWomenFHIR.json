    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "SupplementalDataElementsFHIR4"
    }, {
      "url": "name",
      "valueString": "SDE Sex"
    }, {
      "url": "statement",
      "valueString": "define \"SDE Sex\":\n  case\n      when Patient.gender = 'male' then Code { code: 'M', system: 'http://hl7.org/fhir/v3/AdministrativeGender', display: 'Male' }\n      when Patient.gender = 'female' then Code { code: 'F', system: 'http://hl7.org/fhir/v3/AdministrativeGender', display: 'Female' }\n      else null\n    end"
    }, {
      "url": "displaySequence",
      "valueInteger": 0
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "SDE Sex"
    }, {
      "url": "statement",
      "valueString": "define \"SDE Sex\":\n  SDE.\"SDE Sex\""
    }, {
      "url": "displaySequence",
      "valueInteger": 1
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Numerator"
    }, {
      "url": "statement",
      "valueString": "define \"Numerator\":\n  exists ([\"Observation\": \"Chlamydia Screening\"] ChlamydiaTest\n           where ChlamydiaTest.status in {'preliminary', 'amended', 'corrected','final'}\n           and ChlamydiaTest.value is not null\n             and end of Global.\"Normalize Interval\"(ChlamydiaTest.effective) during \"Measurement Period\")"
    }, {
      "url": "displaySequence",
      "valueInteger": 2
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Qualifying Encounters"
    }, {
      "url": "statement",
      "valueString": "define \"Qualifying Encounters\":\n  ([\"Encounter\": \"Office Visit\"]\n      union [\"Encounter\": \"Preventive Care Services - Established Office Visit, 18 and Up\"]\n      union [\"Encounter\": \"Preventive Care Services-Initial Office Visit, 18 and Up\"]\n      union [\"Encounter\": \"Preventive Care Services, Initial Office Visit, 0 to 17\"]\n      union [\"Encounter\": \"Preventive Care, Established Office Visit, 0 to 17\"]\n      union [\"Encounter\": \"Home Healthcare Services\"]) ValidEncounter\n          \t\twhere ValidEncounter.period during \"Measurement Period\"\n                  \t\tand ValidEncounter.status  = 'finished'"
    }, {
      "url": "displaySequence",
      "valueInteger": 3
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Assessments Identifying Sexual Activity"
    }, {
      "url": "statement",
      "valueString": "define \"Assessments Identifying Sexual Activity\":\n  [\"Observation\": \"Sexually Active\"] SexualActivityAssessment\n      where SexualActivityAssessment.code ~ \"Yes (qualifier value)\"\n          and SexualActivityAssessment.status in {'final', 'amended', 'corrected', 'preliminary'}\n        and end of Global.\"Normalize Interval\"(SexualActivityAssessment.effective) before end of \"Measurement Period\""
    }, {
      "url": "displaySequence",
      "valueInteger": 4
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Diagnoses Identifying Sexual Activity"
    }, {
      "url": "statement",
      "valueString": "define \"Diagnoses Identifying Sexual Activity\":\n  ([\"Condition\": \"Other Female Reproductive Conditions\"]\n          union [\"Condition\": \"Genital Herpes\"]\n          union [\"Condition\": \"Gonococcal Infections and Venereal Diseases\"]\n          union [\"Condition\": \"Inflammatory Diseases of Female Reproductive Organs\"]\n          union [\"Condition\": \"Chlamydia\"]\n          union [\"Condition\": \"HIV\"]\n          union [\"Condition\": \"Syphilis\"]\n          union [\"Condition\": \"Carrier of Predominantly Sexually Transmitted Infection\"]\n          union [\"Condition\": \"Complications of Pregnancy, Childbirth and the Puerperium\"]) SexualActivityDiagnosis\n          where SexualActivityDiagnosis.verificationStatus ~ ToConcept(Global.\"confirmed\")\n              and Global.\"Prevalence Period\"(SexualActivityDiagnosis) overlaps \"Measurement Period\""
    }, {
      "url": "displaySequence",
      "valueInteger": 5
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Active Contraceptive Medications"
    }, {
      "url": "statement",
      "valueString": "define \"Active Contraceptive Medications\":\n  [\"MedicationStatement\": \"Contraceptive Medications\"] ActiveContraceptives\n          where ActiveContraceptives.status = 'active'\n          and Global.\"Normalize Interval\"(ActiveContraceptives.effective) overlaps \"Measurement Period\""
    }, {
      "url": "displaySequence",
      "valueInteger": 6
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Ordered Contraceptive Medications"
    }, {
      "url": "statement",
      "valueString": "define \"Ordered Contraceptive Medications\":\n  [\"MedicationRequest\": \"Contraceptive Medications\"] OrderedContraceptives\n          where OrderedContraceptives.status in {'active','completed'}\n          and OrderedContraceptives.intent.value = 'order'\n              and OrderedContraceptives.authoredOn during \"Measurement Period\""
    }, {
      "url": "displaySequence",
      "valueInteger": 7
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Laboratory Tests Identifying Sexual Activity But Not Pregnancy"
    }, {
      "url": "statement",
      "valueString": "define \"Laboratory Tests Identifying Sexual Activity But Not Pregnancy\":\n  ([\"ServiceRequest\": \"Pap Test\"]\n          union [\"ServiceRequest\": \"Lab Tests During Pregnancy\"]\n          union [\"ServiceRequest\": \"Lab Tests for Sexually Transmitted Infections\"]) LabOrders\n          where LabOrders.status in {'active', 'completed'}\n          and LabOrders.intent.value = 'order'\n          and LabOrders.authoredOn during \"Measurement Period\""
    }, {
      "url": "displaySequence",
      "valueInteger": 8
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Laboratory Tests Identifying Sexual Activity"
    }, {
      "url": "statement",
      "valueString": "define \"Laboratory Tests Identifying Sexual Activity\":\n  ( [\"ServiceRequest\": \"Pregnancy Test\"]\n          union \"Laboratory Tests Identifying Sexual Activity But Not Pregnancy\" ) AllLabOrders\n          where AllLabOrders.status in {'active', 'completed'}\n          and AllLabOrders.intent.value = 'order'\n          and AllLabOrders.authoredOn during \"Measurement Period\""
    }, {
      "url": "displaySequence",
      "valueInteger": 9
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Diagnostic Studies Identifying Sexual Activity"
    }, {
      "url": "statement",
      "valueString": "define \"Diagnostic Studies Identifying Sexual Activity\":\n  [\"ServiceRequest\": \"Diagnostic Studies During Pregnancy\"] SexualActivityDiagnostics\n          where SexualActivityDiagnostics.status in {'active', 'completed'}\n          and SexualActivityDiagnostics.intent.value = 'order'\n          and SexualActivityDiagnostics.authoredOn during \"Measurement Period\""
    }, {
      "url": "displaySequence",
      "valueInteger": 10
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Procedures Identifying Sexual Activity"
    }, {
      "url": "statement",
      "valueString": "define \"Procedures Identifying Sexual Activity\":\n  ( [\"Procedure\": \"Delivery Live Births\"]\n          union [\"Procedure\": \"Procedures During Pregnancy\"]\n          union [\"Procedure\": \"Procedures Involving Contraceptive Devices\"] ) ProceduresForSexualActivity\n          where ProceduresForSexualActivity.status = 'completed'\n          and Global.\"Normalize Interval\"(ProceduresForSexualActivity.performed) during \"Measurement Period\""
    }, {
      "url": "displaySequence",
      "valueInteger": 11
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Initial Population"
    }, {
      "url": "statement",
      "valueString": "define \"Initial Population\":\n  (Patient.gender = 'female'\n    and AgeInYearsAt(start of \"Measurement Period\") between 16 and 24\n  )\n  and exists (\"Qualifying Encounters\")\n  and (exists (\"Assessments Identifying Sexual Activity\")\n    or exists (\"Diagnoses Identifying Sexual Activity\")\n    or exists (\"Active Contraceptive Medications\")\n    or exists (\"Ordered Contraceptive Medications\")\n    or exists (\"Laboratory Tests Identifying Sexual Activity\")\n    or exists (\"Diagnostic Studies Identifying Sexual Activity\")\n    or exists (\"Procedures Identifying Sexual Activity\"))"
    }, {
      "url": "displaySequence",
      "valueInteger": 12
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "ChlamydiaScreeningforWomenFHIR"
    }, {
      "url": "name",
      "valueString": "Denominator"
    }, {
      "url": "statement",
      "valueString": "define \"Denominator\":\n  \"Initial Population\""
    }, {
      "url": "displaySequence",
      "valueInteger": 13
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
    "extension": [ {
      "url": "libraryName",
      "valueString": "SupplementalDataElementsFHIR4"
    }, {
      "url": "name",
      "valueString": "SDE Payer"
    }, {
      "url": "statement",
      "valueString": "define \"SDE Payer\":\n  [Coverage: type in \"Payer\"] Payer\n        return {\n          code: Payer.type,\n          period: Payer.period\n        }"
    }, {
      "url": "displaySequence",
      "valueInteger": 14
    } ]
  }, {
    "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
